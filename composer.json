    "type": "project",
    "require": {
        "themewinter/uninstaller_form": "dev-main",
        "arraytics/tools-sdk": "dev-main",
        "arraytics/plugin-notice-sdk": "dev-main"
    },
    "autoload": {
        "classmap": [
            "base/",
